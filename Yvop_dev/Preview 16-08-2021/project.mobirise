                "height": "44px"
              },
              ".carousel-control-next": {
                "right": "40%",
                "bottom": "5px",
                "top": "auto",
                "left": "auto"
              },
              ".carousel-control-prev": {
                "right": "45%",
                "bottom": "5px",
                "top": "auto",
                "left": "auto"
              }
            },
            "@media screen and (~'-ms-high-contrast: active'), (~'-ms-high-contrast: none')": {
              ".image-element": {
                "min-width": "50%"
              },
              ".media-container-row": {
                "width": "100%"
              }
            },
            "@media (max-width: 991px)": {

                "&:hover": {
                  ".mobi-mbri-left": {
                    "transform": "translateX(-5px)"
                  },
                  ".mobi-mbri-right": {
                    "transform": "translateX(5px)"
                  }
                }
              },
              ".carousel-control-next, .carousel-control-prev": {
                "opacity": "1",

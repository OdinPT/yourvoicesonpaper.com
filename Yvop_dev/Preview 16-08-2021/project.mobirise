              },
              ".mbr-section-title": {
                "margin-bottom": "18px"
              },
              ".slider__image": {
                "margin-left": "0",
                "margin-bottom": "100px"
              },
              ".img-wrap": {
                "width": "100%"
              },
              ".carousel-controls": {
                "display": "none"
              },
              ".carousel-inner": {
                "margin-bottom": "0"
              }
            },
            ".mbr-text": {
              "color": "#000000"
            }
